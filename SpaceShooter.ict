        events:
          - eventKey: OnRoomStart
            code: |-
              this.timer1 = 0.3 // asteroids timer
              this.timer2 = 3 // enemies timer

              this.score = 0
              this.lives = 3

              this.shield_copy = null
              this.powerup_shield_level = 0
            arguments: {}
            lib: core
          - eventKey: Timer1
            code: >-
              this.timer1 = random.range(0.3, 3)


              const selectedCopy = random.dice("Meteor_Grey_Med_01",
              "Meteor_Grey_Big_01")

              templates.copy(selectedCopy, random(camera.width), -100)
            arguments:
              name: Generate Asteroids
              isUi: false
            lib: core
          - eventKey: Timer2
            code: |-
              this.timer2 = random.range(3, 6)

              const selectedCopy = random.dice("Enemy_Red_02", "Enemy_Red_03")

              templates.copy(selectedCopy, random(camera.width), -100)
            arguments:
              name: Generate Enemies
              isUi: false
            lib: core
          - eventKey: Timer3
            code: >-
              const restartButton = rooms.current.children.find(child =>
              child.template === 'ButtonRestart')

              const backButton = rooms.current.children.find(child =>
              child.template === 'ButtonBackToMain')


              restartButton.x = restartButton.finalX

              backButton.x = backButton.finalX


              tween.value({
                  from: restartButton.finalY + 10,
                  to: restartButton.finalY,
                  duration: 400,
                  curve: tween.easeOutCubic
              }, newValue => {
                  restartButton.y = newValue
              })


              u.wait(70).then(() => {
                  tween.value({
                      from: backButton.finalY + 10,
                      to: backButton.finalY,
                      duration: 400,
                      curve: tween.easeOutCirc
                  }, newValue => {
                      backButton.y = newValue
                  })
              })
            arguments:
              name: Game Over
              isUi: false
            lib: core
          - eventKey: OnActionPress
            code: |-
              if (rooms.current.paused) {
                  rooms.current.paused = false

                  pixiApp.ticker.speed = 1

                  rooms.remove(this.pauseScreen)
              } else {
                  rooms.current.paused = true

                  pixiApp.ticker.speed = 0

                  !this.pauseMenu && (this.pauseScreen = rooms.append("UI_Pause"))
              }
            arguments:
              action: Pause
            lib: core
