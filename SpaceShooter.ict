      - type: template
        baseClass: AnimatedSprite
        name: PlayerShip_02_Blue
        depth: 1
        texture: QgKWGTDCr8HWdT
        playAnimationOnStart: false
        loopAnimation: true
        animationFPS: 30
        visible: true
        events:
          - eventKey: OnStep
            lib: core
            code: >-
              this.hspeed = this.moveSpeed * actions.MoveX.value

              this.vspeed = this.moveSpeed * actions.MoveY.value


              this.x = Math.max(this.textureHalfWidth, Math.min(this.x,
              camera.width - this.textureHalfWidth))

              this.y = Math.max(this.textureHalfHeight, Math.min(this.y,
              camera.height - this.textureHalfHeight))


              this.burstEmitter.frozen = rooms.current.paused


              backgrounds.list["BG_Blue"][0].shiftX = 20 - this.x /
              rooms.current.width * 40


              this.move()
            arguments: {}
          - eventKey: OnCreate
            code: |-
              rooms.current.player = this

              this.moveSpeed = 480

              this.invincible = false
              this.can_shoot = true

              this.powerup_shot = 1

              this.textureHalfWidth = this.texture.width * 0.5
              this.textureHalfHeight = this.texture.height * 0.5

              this.burstEmitter = emitters.follow(this, "Burst Emitter", {
                  position: {
                      x: 0,
                      y: 40
                  },
                  depth: -1
              })

              const fragment = `
                  varying vec2 vTextureCoord;
                  uniform vec2 uMapSize;
                  uniform sampler2D uSampler;
                  uniform vec4 inputPixel;
                  uniform highp vec4 outputFrame;
                  uniform float uEffect;

                  #define EFFECT_COLOR vec3(1.5, 0.1, 0.05)

                  vec2 getUV(vec2 coord) {
                      return coord * inputPixel.xy / outputFrame.zw;
                  }

                  vec3 blendAdd(in vec3 base, in vec3 blend) {
                      return min(base + blend, vec3(1.));
                  }

                  vec3 blendAdd(in vec3 base, in vec3 blend, in float opacity) {
                      return (blendAdd(base, blend) * opacity + base * (1. - opacity));
                  }

                  void main(void) {
                      vec2 uv = getUV(vTextureCoord) * 2.0 - 1.0;
                      uv.x *= uMapSize.x / uMapSize.y;

                      float effectRadius = mod(uEffect * 3.0, 3.0);
                      float d = distance(vec2(0.0), uv);
                      d = smoothstep(0.3 * effectRadius, 1.0*effectRadius, d);
                      vec3 effectColor = EFFECT_COLOR*d;
                      float effectAlpha = 1.0 - step(1.0, d);

                      vec4 tex0 = texture2D(uSampler, vTextureCoord);

                      vec3 color = blendAdd(tex0.rgb, effectColor, effectAlpha*2.0);

                      gl_FragColor = vec4(color, 1.0) * tex0.a;
                  }
              `;

              const uniforms = {
                  uMapSize: [
                      this.texture.width,
                      this.texture.height
                  ],
                  uEffect: 0
              }

              this.fx = ct.filters.custom(this, undefined, fragment, uniforms);
            arguments: {}
            lib: core
          - eventKey: OnActionPress
            code: |-
              if (!this.can_shoot) return
              if (rooms.current.paused) return

              switch (this.powerup_shot) {
                  case 1:
                      templates.copy("Laser_Blue_01", this.x, this.y)
                      break
                  case 2:
                      templates.copy("Laser_Blue_01", this.x + 30, this.y + 5, {
                          shoot_dir: 280,
                          angle: 10
                      })

                      templates.copy("Laser_Blue_01", this.x - 30, this.y + 5, {
                          shoot_dir: 260,
                          angle: -10
                      })
                      break
                  case 3:
                      templates.copy("Laser_Blue_01", this.x, this.y)

                      templates.copy("Laser_Blue_01", this.x + 30, this.y + 5, {
                          shoot_dir: 280,
                          angle: 10
                      })

                      templates.copy("Laser_Blue_01", this.x - 30, this.y + 5, {
                          shoot_dir: 260,
                          angle: -10
                      })
                      break
              }

              sounds.play("SFX Shoot")
            arguments:
              action: Shoot
            lib: core
          - eventKey: collisionCGroup
            code: |-
              if (this.invincible) return

              other.kill = true
              emitters.fire("Smoke Emitter", other.x, other.y)
              sounds.play("SFX EnemyDie")

              rooms.current.lives--

              if (rooms.current.lives <= 0) {
                  sounds.play("SFX EnemyDie")
                  emitters.fire("Smoke Emitter", this.x, this.y)

                  this.kill = true

                  rooms.current.timer3 = 1
              } else {
                  sounds.play("SFX PlayerHit")

                  tween.value({
                      from: 0,
                      to: 1,
                      duration: 500
                  }, newValue => {
                      this.fx.uniforms.uEffect = newValue
                  })
              }

              this.invincible = true
              this.can_shoot = false

              u.wait(1000).then(() => {
                  this.invincible = false
                  this.can_shoot = true
              })
            arguments:
              group: Hostile
            lib: place
          - eventKey: OnDraw
            code: // this.fx.uniforms.uTime += pixiApp.ticker.deltaTime * 0.1
            arguments: {}
            lib: core
        extends:
          cgroup: Player
