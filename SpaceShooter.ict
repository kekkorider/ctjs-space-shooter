            readableName: Final X
            required: true
            array: false
            fixedLength: 0
          - type: number
            name: finalY
            readableName: Final Y
            required: true
            array: false
            fixedLength: 0
        extendTypes: ''
        lastmod: 1718876409993
        uid: Gq756PDKHMBn7G
      - type: behavior
        name: Shield Behavior
        behaviorType: template
        events:
          - eventKey: collisionCGroup
            code: |-
              other.kill = true

              {   
                  // Reduce the level of the powerup
                  rooms.current.powerup_shield_level = Math.max(0, rooms.current.powerup_shield_level - 1)

                  // Remove the old copy
                  rooms.current.removeChild(rooms.current.shield_copy)

                  if (rooms.current.powerup_shield_level === 0) {
                      // Set to `null` if the shield is gone...
                      rooms.current.shield_copy = null
                  } else {
                      // ... otherwise add a new copy of the current shield's level
                      rooms.current.shield_copy = templates.copy(
                          `Shield_0${rooms.current.powerup_shield_level}`,
                          rooms.current.player.x,
                          rooms.current.player.y
                      )
                  }
              }
            arguments:
              group: Hostile
            lib: place
          - eventKey: OnDraw
            code: |-
              this.x = rooms.current.player.x
              this.y = rooms.current.player.y
            arguments: {}
            lib: core
        specification: []
        extendTypes: ''
        lastmod: 1719352449820
        uid: WNJpDLk9TR3jgQ
