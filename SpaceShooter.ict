    entries:
      - type: behavior
        name: Asteroids Behavior
        behaviorType: template
        events:
          - eventKey: OnCreate
            code: |-
              this.speed = random.range(50, 100)
              this.direction = random.range(60, 120)
            arguments: {}
            lib: core
          - eventKey: OnStep
            code: |-
              this.y > camera.height + this.texture.height && (this.kill = true)

              this.angle += 100 * u.time

              this.move()
            arguments: {}
            lib: core
          - eventKey: collisionCGroup
            code: |-
              other.kill = true
              this.kill = true

              emitters.fire("Smoke Emitter", this.x, this.y)
              sounds.play("SFX AsteroidDie")
            arguments:
              group: PlayerWeapons
            lib: place
        specification: []
        extendTypes: ''
        lastmod: 1718927114009
        uid: 1b9c95zrCknK7b
      - type: behavior
        name: Enemies Behavior
        behaviorType: template
        events:
          - eventKey: OnCreate
            code: |-
              this.speed = 180
              this.direction = random.range(70, 110)

              this.timer1 = 1
            arguments: {}
            lib: core
          - eventKey: OnStep
            code: |-
              this.y > camera.height + this.texture.height && (this.kill = true)

              this.move()
            arguments: {}
            lib: core
          - eventKey: Timer1
            code: >-
              this.timer1 = 3

              templates.copy(this.shoot_label, this.x, this.y +
              this.texture.height * 0.5)
            arguments:
              name: Shoot
              isUi: false
            lib: core
          - eventKey: OnDestroy
            code: >-
              const dropPowerUp = random.dice(0, 1)


              if (!dropPowerUp) return


              const powerup = random.dice("Powerup_Green_Shield",
              "Powerup_Red_Bolt")

              templates.copy(powerup, this.x, this.y)
            arguments: {}
            lib: core
          - eventKey: collisionCGroup
            code: |-
              other.kill = true
              this.kill = true

              rooms.current.score += this.score_on_kill ?? 10

              emitters.fire("Smoke Emitter", this.x, this.y)
              sounds.play("SFX EnemyDie")
            arguments:
              group: PlayerWeapons
            lib: place
        specification: []
        extendTypes: ''
        lastmod: 1718930775367
        uid: tFn59wC2D4JHTN
      - type: behavior
        name: Enemy Laser Behavior
        behaviorType: template
        events:
          - eventKey: OnCreate
            code: |-
              this.speed = 480
              this.direction = 90
            arguments: {}
            lib: core
          - eventKey: OnStep
            code: |-
              this.y > camera.height + this.texture.height && (this.kill = true)

              this.move()
            arguments: {}
            lib: core
        specification: []
        extendTypes: ''
        lastmod: 1718927146849
        uid: JR5J52dnzK2hgn
      - type: behavior
        name: Powerup Behavior
        behaviorType: template
        events:
          - eventKey: OnCreate
            code: |-
              this.speed = 80
              this.direction = 90
            arguments: {}
            lib: core
          - eventKey: OnStep
            code: this.kill = this.y > camera.height + 80
            arguments: {}
            lib: core
          - eventKey: collisionCGroup
            code: |-
              switch (this.powerup_name) {
                  case "Shield":
                      // Give 1000pts if shield level is 3
                      rooms.current.powerup_shield_level === 3 &&
                          (rooms.current.score += 1000)

                      // Increase shield level (capped to 3)
                      rooms.current.powerup_shield_level = Math.min(3, rooms.current.powerup_shield_level + 1)

                      // Destroy the current shield copy if any
                      rooms.current.shield_copy &&
                          rooms.current.removeChild(rooms.current.shield_copy)

                      // Add the new shield
                      rooms.current.shield_copy = templates.copy(`Shield_0${rooms.current.powerup_shield_level}`, 0, 0)
                      break
                  case "Shots":
                      // Give 1000pts if powerup level is 3
                      other.powerup_shot === 3 &&
                          (rooms.current.score += 1000)

                      // Increase powerup level (capped to 3)
                      other.powerup_shot = Math.max(1, Math.min(3, other.powerup_shot + 1))
                      break
              }

              sounds.play("SFX Powerup")

              this.kill = true
            arguments:
              group: Player
            lib: place
        specification: []
        extendTypes: ''
        lastmod: 1719353771174
        uid: kGtLHT7pMQFGNH
      - type: behavior
        name: Button Behavior
        behaviorType: template
        events:
          - eventKey: OnPointerEnter
            code: |-
              tween.add({
                  obj: this.scale,
                  fields: {
                      x: 0.55,
                      y: 0.55,
                  },
                  duration: 250,
                  isUi: true
              })
            arguments: {}
            lib: core
          - eventKey: OnPointerLeave
            code: |-
              tween.add({
                  obj: this.scale,
                  fields: {
                      x: 0.5,
                      y: 0.5,
                  },
                  duration: 250,
                  isUi: true
              })
            arguments: {}
            lib: core
